<div class="container">
  <div class=row>
    <table>
      <thead>
        <tr>
          <th>id</th>
          <th>plate</th>
          <th>model</th>
          <th>buyer</th>
        </tr>
      </thead>
      <tbody>
        <% @cars.each do |car| %>
        <tr>
          <td><%= car.id %></td>
          <td><%= car.plate %></td>
          <td><%= car.model %></td>
          <td><%= car.buyer %></td>
        </tr>
        <%- end %>
      </tbody>
    </table>
  </div>
</div>